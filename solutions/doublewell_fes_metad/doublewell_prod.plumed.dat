UNITS ENERGY=kcal/mol

d1: DISTANCE ATOMS=1,2 
ff: MATHEVAL ARG=d1 PERIODIC=NO FUNC=0.2*(((x-10)^2)*((x-20)^2))
bb: BIASVALUE ARG=ff

metad: METAD ARG=d1 PACE=500 HEIGHT=0.1 SIGMA=2.5 FILE=_OUTPREFIX_.hills BIASFACTOR=10 TEMP=300.0 GRID_WFILE=_OUTPREFIX_.grid GRID_MIN=0 GRID_MAX=30 GRID_BIN=251 GRID_WSTRIDE=10000

F: RESTRAINT ARG=d1 AT=0.0 KAPPA=0 SLOPE=_FORCE_

DUMPATOMS STRIDE=2000 FILE=_OUTPREFIX_.xyz ATOMS=1-2,

PRINT ARG=* FILE=_OUTPREFIX_.metad.dat STRIDE=100

